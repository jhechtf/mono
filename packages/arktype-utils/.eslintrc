{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@jhecht/eslint-plugin"
  ],
  "extends": [
    "plugin:@jhecht/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "arrow-parens": ["warn", "always"],
    "@jhecht/max-static-destructure-depth": ["error", 3]
  }
}